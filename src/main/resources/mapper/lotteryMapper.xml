<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.szxfd.springboot.web.dao.ILotteryMapper" >

    <resultMap id="BaseResultMap" type="com.szxfd.springboot.web.entity.Lottery" >
        <id column="id" property="id" jdbcType="INTEGER" />
        <result column="lottery_name" property="lotteryName" jdbcType="VARCHAR" />
        <result column="lottery_desc" property="lotteryDesc" jdbcType="VARCHAR" />
    </resultMap>
    <sql id="Base_Column_List" >
        id, lottery_name, lottery_desc
    </sql>
    <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.Integer" >
        select
        <include refid="Base_Column_List" />
        from lottery
        where id = #{id,jdbcType=INTEGER}
    </select>

</mapper>